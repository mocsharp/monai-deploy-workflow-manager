{
  "WorkflowManager": {
    "TaskManager": {
      "concurrency": 1,
      "plug-ins": {
        "argo": "Monai.Deploy.WorkflowManager.TaskManager.Argo.ArgoPlugin, Monai.Deploy.WorkflowManager.TaskManager.Argo"
      }
    },
    "messaging": {
      "publisherSettings": {
        "endpoint": "localhost",
        "username": "admin",
        "password": "admin",
        "virtualHost": "monaideploy",
        "exchange": "monaideploy"
      },
      "subscriberSettings": {
        "endpoint": "localhost",
        "username": "admin",
        "password": "admin",
        "virtualHost": "monaideploy",
        "exchange": "monaideploy",
        "exportRequestQueue": "export_tasks"
      }
    },
    "storage": {
      "settings": {
        "bucket": "monaideploy",
        "endpoint": "localhost:9000",
        "accessKey": "minioadmin",
        "accessToken": "minioadmin",
        "securedConnection": false,
        "region": "eu-west-2",
        "credentialServiceUrl": "http://localhost:9000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker": "Error",
      "Monai": "Debug"
    },
    "Console": {
      "FormatterOptions": {
        "ColorBehavior": "Disabled",
        "IncludeScopes": true,
        "SingleLine": false,
        "TimestampFormat": " HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Http2": {
        "Url": "https://localhost:7000",
        "Protocols": "Http2"
      }
    }
  },
  "AllowedHosts": "*"
}
